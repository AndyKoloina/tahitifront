mixin card-map-contact
  .card-map-contact
    .card-place
      p
        img.icone-sm.icone-marker(src='../img/icons/marker.svg' alt='marker')
      p
        strong.text-uppercase.map-title face tony
      p
        i.map-place Face au McDonald de Papeete
      p
        ul
          li
            strong.map-date Lundi à vendredi 08h00 à 17h00
            span.map-hour  08h00 à 17h00
          li
            strong.map-date Samedi
            span  08h00 à 17h00

    .card-contact
      p
        img.icone-sm.icone-marker(src='../img/icons/phone-blue.svg' alt='marker')
      p.d-flex.flex-column
        span Contactez l'equipe Tahiti phone au
        strong.number-phone 39 50*
      p
        ul
          li
            strong.map-date lundi au jeudi
            span.map-hour  08h00 à 17h00
          li
            strong.map-date Vendredi
            span.map-hour  08h00 à 17h00
          li
            strong.map-date Samedi
            span.map-hour  08h00 à 17h00
